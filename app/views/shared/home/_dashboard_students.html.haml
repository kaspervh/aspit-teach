.dashboard-students
  .dashboard-titlebox
    %h3 Elever  
  %table.table
    %thead.thead-inverse
      %tr
        %th Navn
        %th Dagbogsnotater
        %th Målsætninger
      
    %tbody
      - @grade_students.each do |student|
        %tr
          %td 
            = student.username
          %td 
            = student.diary_entries.count
          %td 
            = student.student_goals.count
  = link_to "Dagbogsnotater ", user_diary_entries_path(@current_user.id), class: " btn btn-default"
  = link_to "Målsætninger", user_student_goals_path(@current_user.id), class: "btn btn-default"