<h3>Brugere</h3>

    <tr>
      <th>Brugernavn</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><strong><%= user.username %></strong></td>
        <td><strong><%= user.email %></strong></td>
        
        
        <td><%= link_to 'Vis', user, class: "btn btn-secondary" %>
        <%= link_to 'Rediger', edit_user_path(user), class: "btn btn-secondary" %>
        <%= link_to 'Slet', user, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Ny bruger', new_user_path, class: "btn btn-secondary" %>
